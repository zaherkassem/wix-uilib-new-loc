{
  "name": "editor-ui-lib",
  "version": "0.0.6",
  "description": "wix ui controls for third party applications",
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  },
  "scripts": {
    "lint": "eslint js/**",
    "test": "karma start --single-run --browsers PhantomJS --reporters teamcity",
    "clean": "rimraf lib",
    "dev-server-react": "webpack-dev-server --config webpack.config.js",
    "dev-server-jquery": "webpack-dev-server --config webpack.ui-lib-jquery.config.js",
    "dev-server-angular": "webpack-dev-server --config webpack.editor-ui-lib-angular.config.js",
    "build-react": "webpack --config webpack.config.min.js",
    "build-react-debug": "webpack --config webpack.config.js",
    "build-jquery": "webpack --config webpack.ui-lib-jquery.config.min.js",
    "build-jquery-debug": "webpack --config webpack.ui-lib-jquery.config.js",
    "build-angular": "webpack --config webpack.editor-ui-lib-angular.config.min.js",
    "build-angular-debug": "webpack --config webpack.editor-ui-lib-angular.config.js",
    "build-all": "npm run build-react && npm run build-react-debug && npm run build-jquery && npm run build-jquery-debug && npm run build-angular && npm run build-angular-debug",
    "build": "npm run build-all",
    "release": "wnpm-release"
  },
  "main": "lib/editor-ui-lib.min.js",
  "files": [
    "lib"
  ],
  "author": "sdk-dev@wix.com",
  "license": "MIT",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/wix/editor-ui-lib-docs.git"
  },
  "devDependencies": {
    "angular": "^1.4.4",
