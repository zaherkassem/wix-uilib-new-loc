    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.16.0",
    "es6-promise": "3.2.1",
    "eslint": "2.5.3",
    "eslint-config-wix-editor": "0.2.3",
    "eslint-loader": "1.3.0",
    "expose-loader": "^0.7.0",
    "express": "4.10.2",
    "jasmine-core": "^2.3.4",
    "karma": "1.3.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "^0.1.0",
    "karma-jasmine": "^0.3.6",
