    "lodash": "4.16.1",
    "node-sass": "^3.2.0",
    "nodemon": "1.3.7",
    "phantomjs": "^1.9.17",
    "postcss-loader": "^0.13.0",
    "react": "^15.3.1",
    "react-cli": "^0.2.0",
    "react-dom": "^15.3.1",
    "react-templates": "0.5.2",
    "react-templates-loader": "0.5.1",
    "rimraf": "^2.3.4",
    "sass-loader": "git+https://github.com/yaarams/sass-loader.git",
